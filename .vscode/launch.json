    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch sequential bfs tests",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/sequential_bfs_tests",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
